{
  "name": "web-shared-ui-spinner",
  "projectType": "library",
  "sourceRoot": "libs/web/shared/ui/spinner/src",
  "prefix": "elektra-nx",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/web/shared/ui/spinner"],
      "options": {
        "jestConfig": "libs/web/shared/ui/spinner/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/web/shared/ui/spinner/**/*.ts", "libs/web/shared/ui/spinner/**/*.html"]
      }
    }
  },
  "tags": ["type:ui", "scope:web"]
}
